<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd" version="2.0">
    <namespace>http://wicket.jsf/functions</namespace>

    <tag>
		<tag-name>url</tag-name>
		<handler-class>wicket.jsf.taglib.WicketJSFURL</handler-class>
    </tag>
    
    <function>
        <function-name>url</function-name>
        <function-class>wicket.jsp.el.WicketELURL</function-class>
        <function-signature>java.lang.String url(java.lang.String)</function-signature>
    </function>

    <function>
        <function-name>urlWithQuery</function-name>
        <function-class>wicket.jsp.el.WicketELURL</function-class>
        <function-signature>java.lang.String urlWithQuery(java.lang.String, java.lang.String)</function-signature>
    </function>    
</facelet-taglib>